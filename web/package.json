{
  "name": "@cms-eapd/web",
  "version": "0.0.1",
  "description": "APD app for CMS HITECH program",
  "main": "app.js",
  "exports": "./index.js",
  "scripts": {
    "lint": "eslint 'src/**/*.js'",
    "scaffold": "rm -rf dist && mkdir dist && cp -r src/static dist/",
    "build": "yarn run scaffold && NODE_ENV=production webpack --config webpack.config.js",
    "start": "yarn run scaffold && webpack serve --config webpack.config.dev.js --port ${WEB_PORT:-8001}",
    "npm-check": "npx npm-check-updates",
    "make-state-icons": "babel-node scripts/stateIcons.js && cp src/data/states-small/* src/static/img/states/",
    "test:generate-output": "TZ=utc jest --json --outputFile=.jest-test-results.json -i --silent",
    "test": "DEBUG_PRINT_LIMIT=10000 TZ=utc jest --collectCoverage",
    "test-watch": "DEBUG_PRINT_LIMIT=10000 TZ=utc yarn run test:generate-output -- --watch",
    "prebuild:storybook": "yarn run scaffold && yarn run test:generate-output",
    "build:storybook": "build-storybook -o docs-build",
    "deploy-storybook": "storybook-to-ghpages --out=docs-build",
    "storybook": "start-storybook -p 6006",
    "deploy-chromatic": "chromatic --storybook-build-dir docs-build --no-interactive --only-changed",
    "stryker": "TZ=utc stryker run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Enterprise-CMCS/eAPD.git"
  },
  "license": "CC0-1.0",
